<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".JuegoActivity">
    <ImageView
        android:background="#080013"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </ImageView>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/vengadores">
    </ImageView>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TableLayout
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#275D00E3">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:onClick="onClick"
                    android:id="@+id/image11"
                    android:layout_weight="1"
                    android:layout_margin="3dp"
                    android:layout_width="100dp"
                    android:maxWidth="100dp"
                    android:minWidth="100dp"
                    android:layout_height="100dp"
                    android:maxHeight="100dp"
                    android:minHeight="100dp"
                    android:background="#9EF8F5F5"
                    android:src="@drawable/fondo">
                </ImageView>
                <ImageView
                    android:onClick="onClick"
                    android:id="@+id/image12"
                    android:layout_weight="1"
                    android:layout_margin="3dp"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="#9EF8F5F5"
                    android:src="@drawable/fondo">
                </ImageView>
                <ImageView
                    android:onClick="onClick"
                    android:id="@+id/image13"
                    android:layout_weight="1"
                    android:layout_margin="3dp"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="#9EF8F5F5"
                    android:src="@drawable/fondo">
                </ImageView>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >
                <ImageView
                    android:onClick="onClick"
                    android:id="@+id/image21"
                    android:layout_weight="1"
                    android:layout_margin="3dp"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="#9EF8F5F5"
                    android:src="@drawable/fondo">
                </ImageView>
                <ImageView
                    android:onClick="onClick"
                    android:id="@+id/image22"
                    android:layout_weight="1"
                    android:layout_margin="3dp"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="#9EF8F5F5"
                    android:src="@drawable/fondo">
                </ImageView>
                <ImageView
                    android:onClick="onClick"
                    android:id="@+id/image23"
                    android:layout_weight="1"
                    android:layout_margin="3dp"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="#9EF8F5F5"
                    android:src="@drawable/fondo">
                </ImageView>
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent" >
                <ImageView
                    android:onClick="onClick"
                    android:id="@+id/image31"
                    android:layout_weight="1"
                    android:layout_margin="3dp"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="#9EF8F5F5"
                    android:src="@drawable/fondo">
                </ImageView>
                <ImageView
                    android:onClick="onClick"
                    android:id="@+id/image32"
                    android:layout_weight="1"
                    android:layout_margin="3dp"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="#9EF8F5F5"

                    android:src="@drawable/fondo">
                </ImageView>
                <ImageView
                    android:onClick="onClick"
                    android:id="@+id/image33"
                    android:layout_weight="1"
                    android:layout_margin="3dp"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="#9EF8F5F5"
                    android:src="@drawable/fondo">
                </ImageView>
            </TableRow>

        </TableLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>